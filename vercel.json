{
  "version": 2,
  "buildCommand": "dotnet publish -c Release -o .vercel/output/static",
  "outputDirectory": ".vercel/output/static",
  "installCommand": "curl -sSL https://dot.net/v1/dotnet-install.sh | bash /dev/stdin --version 8.0.100",
  "builds": [
    {
      "src": "**/*.cs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/$1"
    }
  ]
 }

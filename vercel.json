{
  "version": 2,
  "builds": [
    {
      "src": "first_asp_app.csproj",
      "use": "@vercel/dotnet",
      "config": { "framework": "net8.0" }
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}

{
  "version": 2,
  "builds": [
    {
      "src": "**/*.csproj",
      "use": "@vercel/static-build",
      "config": {
        "command": "dotnet publish -c Release -o publish",
        "output": "publish"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/publish/$1"
    }
  ],
  "env": {
    "ASPNETCORE_ENVIRONMENT": "Production"
  }
}
